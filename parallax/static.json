{
    "routes": [
      {
        "src": "/(.*).(html|css|js|png|jpg|jpeg|gif|ico|svg|txt)",
        "dest": "/public/$1.$2"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "/favicon.ico"
      },
      {
        "handle": "filesystem"
      }
    ]
  }
  